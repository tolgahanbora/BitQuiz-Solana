{
  "expo": {
    "name": "BitQuiz",
    "slug": "BitQuiz",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/bitquiz-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2A175B"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tolgahanbora.BitQuiz"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/bitquiz-foreground.png",
        "backgroundColor": "#2A175B"
      },
      "package": "com.tolgahanbora.BitQuiz"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "64db64b7-76bc-420d-82ba-30bc0ee56609"
      }
    }
  },
  "react-native-google-mobile-ads": {
    "android_app_id": "ca-app-pub-2590549735225636~6253860342",
    "ios_app_id": "ca-app-pub-2590549735225636~6253860342"
    
  }
}
