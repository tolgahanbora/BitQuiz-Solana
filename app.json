{
  "expo": {
    "name": "BitQuiz",
    "slug": "BitQuiz",
    "version": "2.2.0",
    "orientation": "portrait",
    "icon": "./assets/bitquiz-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/bitquiz-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2A175B"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.tolgahanbora.BitQuizMobile"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 33,
            "targetSdkVersion": 33,
            "buildToolsVersion": "33.0.0"
          },
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/transparan-bitquiz.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.tolgahanbora.BitQuizMobile",
      "permissions": [
        "com.android.vending.BILLING",
        "android.permission.INTERNET"
      ],
      "versionCode": 21
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "64db64b7-76bc-420d-82ba-30bc0ee56609"
      }
    }
  },
  "react-native-google-mobile-ads": {
    "android_app_id": "ca-app-pub-2590549735225636~6253860342"
  }
}
